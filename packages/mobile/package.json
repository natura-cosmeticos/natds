{
  "name": "@naturacosmeticos/natds-rn",
  "version": "0.0.1",
  "description": "Natura Design System - A collection of components used by Natura Cosmeticos",
  "keywords": [
    "design-system"
  ],
  "author": "Vizir Software Studio <contato@vizir.com.br>",
  "contributors": [
    "Vicente Lyrio <vicente.lyrio@vizir.com.br>",
    "Vagner Saraiva <vagner@vizir.com.br>",
    "Matheus Robert Lichtnow <matheus@vizir.com.br>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/natura-cosmeticos/natds/packages/mobile",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/natura-cosmeticos/natds.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 8\"",
    "start": "react-native start",
    "test:unit": "jest",
    "test:unit:ci": "yarn test:unit",
    "test:unit:coverage": "yarn test:unit --collectCoverage --verbose false",
    "test:unit:coverage:open": "open coverage/lcov-report/index.html",
    "test:unit:generate": "yarn test:unit --json --outputFile=.jest-test-results.json || true",
    "test:unit:watch": "jest --watch",
    "test": "yarn test:unit:coverage -u",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "build:clean": "rm -rf ./dist",
    "build": "yarn build:clean && tsc -p ./tsconfig.json",
    "watch": "yarn build:clean && tsc -p ./tsconfig.json --watch",
    "verify": "yarn lint && yarn test"
  },
  "dependencies": {
    "@naturacosmeticos/natds-styles": "^0.0.1",
    "@types/react-native-vector-icons": "^6.4.4",
    "proptypes": "^1.1.0",
    "react-native-paper": "^3.2.1",
    "react-native-vector-icons": "^6.6.0"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-native": "^0.61.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "24.0.23",
    "@types/react": "16.9.11",
    "@types/react-native": "^0.60.17",
    "@types/react-test-renderer": "16.9.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "core-js": "3.4.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom": "^24.9.0",
    "jsdom": "^15.2.1",
    "metro-react-native-babel-preset": "^0.57.0",
    "react-dom": "^16.10.2",
    "react-native": "^0.61.2",
    "react-native-custom-tabs": "^0.1.7",
    "react-native-device-info": "^5.3.1",
    "react-native-mock-render": "^0.1.7",
    "react-native-safari-view": "^2.1.0",
    "react-test-renderer": "16.11.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "yarn": "^1.19.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
