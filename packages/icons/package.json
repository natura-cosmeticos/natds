{
  "name": "@naturacosmeticos/natds-icons",
  "displayName": "NatDS Icons",
  "version": "0.7.8",
  "private": false,
  "description": "A collection of icons for Natura Design System",
  "keywords": [
    "design-system",
    "icons",
    "natds",
    "natura"
  ],
  "homepage": "https://github.com/natura-cosmeticos/natds-js/tree/master/packages/icons/README.md",
  "bugs": {
    "url": "https://github.com/natura-cosmeticos/natds-js/issues"
  },
  "license": "ISC",
  "author": "Natura Cosméticos <designsystem@natura.net>",
  "contributors": [
    "Daniel Castro <daniel.castro@vizir.com.br>",
    "Jimmy Andrade <jimmy.andrade@thoughtworks.com>",
    "Matheus Robert Lichtnow <matheus@vizir.com.br>",
    "Vagner Saraiva <vagner@vizir.com.br>",
    "Vicente Lyrio <vicente.lyrio@vizir.com.br>",
    "Vinícius Ponciano <vinicius.ponciano@thoughtworks.com>"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "browser": "dist/umd/index.js",
  "jsdelivr": "dist/umd/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "unpkg": "dist/umd/index.js",
  "repository": {
    "directory": "packages/icons",
    "type": "git",
    "url": "https://github.com/natura-cosmeticos/natds.git"
  },
  "scripts": {
    "build": "yarn build:deps && tsc -p ./tsconfig.json && yarn build:copyfiles && yarn rollup:build",
    "build:clean": "rm -rf dist && rm -rf src/assets/cleaned/* && rm -rf src/fonts/*",
    "build:cleansvgs": "node ./scripts/svgCleaner.js",
    "build:copyfiles": "cp -r ./src/fonts ./dist && cp ./src/*.css ./dist",
    "build:csstypes": "tcm -o src src",
    "build:deps": "yarn build:clean && yarn build:cleansvgs && yarn build:fonts && yarn build:csstypes",
    "build:fonts": "node ./scripts/svgToFont.js",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "rollup:build": "rollup -c",
    "start": "yarn build --watch",
    "verify": "yarn lint",
    "verify:ci": "yarn verify",
    "watch": "yarn start"
  },
  "config": {},
  "devDependencies": {
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-typescript": "4.1.2",
    "@rollup/plugin-url": "5.0.1",
    "codecov": "3.7.0",
    "glob": "7.1.6",
    "rollup": "2.17.1",
    "svgo": "1.3.2",
    "typed-css-modules": "0.6.4",
    "typescript": "3.9.5",
    "webfont": "9.0.0",
    "yarn": "1.22.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
